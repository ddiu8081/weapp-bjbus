/**app.wxss**/
page {
  height: 100%;
  background: #f3f3f3;
  font-size: 28rpx;
}

.container {
  background: #f3f3f3;
  position: relative;
  min-height: 100%;
  box-sizing: border-box;
  font-size: 28rpx;
}

.container-mt {
  padding-top: 108rpx;
}

.container-white {
  background: #ffffff;
}

.navigator-hover {
	background: #f9f9f9;
}

.clear-btn {
  background-color: transparent;
  margin: 0;
  text-align: left;
  border: none !important;
  color: #666666;
  padding: 0;
  line-height: 1;
  border-radius: 0;
}

.title-square {
  display: inline-block;
  margin-right: 10rpx;
  background: #FFD45A;
  width: 14rpx;
  height: 28rpx;
  border-radius: 7rpx;
}

.article {
  font-size: 26rpx;
  padding: 30rpx 50rpx;
  color: #909399;
}

.article text {
  display: block;
  margin-bottom: 20rpx;
}

.article .h1 {
  display: inline-block;
  font-size: 34rpx;
  margin: 20rpx 0 14rpx;
  color: #303133;
  box-shadow: inset 0 -26rpx rgb(255, 238, 193);
}
